select * from REVIEW_BOARD


drop table review_board
select * from review_board
delete REVIEW_BOARD
///이게 진짜입니다
create table review_board(
review_id number PRIMARY KEY,
review_title varchar2(100),
member_id varchar2(30),
review_date date,
review_content varchar2(2048),
review_available number,
review_image_1 varchar2(30),
review_image_2 varchar2(30),
review_image_3 varchar2(30),
review_image_4 varchar2(30),
review_image_5 varchar2(30)
)

create sequence seq_review_id
start with 1
maxvalue 9999
minvalue 0
increment by 1
cycle
nocache;
